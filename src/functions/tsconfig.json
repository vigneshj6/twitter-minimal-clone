{
   "compileOnSave": false,
   "compilerOptions": {
      "types": ["node"],
      "lib": [
         "ES2019"
      ],
      "target": "ES2017",
      "module": "commonjs",
      "rootDir": "..",
      "outDir": "../../dist-functions",
      "declaration": false,
      "inlineSources": false,
      "strict": false,
      "skipLibCheck": true,
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "baseUrl":"."
   },
   "include": [
      "lib/**/*.ts",
      "src/**/*.ts",
      "*.ts"
   ],
   "exclude": [
      "node_modules"
   ],
   "typeRoots": [
      "../node_modules/@types/index.d.ts"
   ]
 }